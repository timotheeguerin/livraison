{
  "name": "livraison",
  "version": "0.0.6",
  "description": "",
  "keywords": [
    "deb",
    "installer",
    "livraison",
    "msi",
    "packer"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timotheeguerin/livraison.git"
  },
  "license": "MIT",
  "author": "Timothee Guerin",
  "type": "module",
  "bin": {
    "livraison": "cmd/cli.js"
  },
  "files": [
    "cmd/cli.js",
    "dist/"
  ],
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json && cargo build --package livraison --target wasm32-wasip1 --release && cp ../../target/wasm32-wasip1/release/livraison.wasm dist/",
    "verify": "node ./cmd/cli.js --version",
    "watch": "tsc -p ./tsconfig.build.json --watch"
  },
  "dependencies": {},
  "devDependencies": {},
  "packageManager": "pnpm@10.5.2",
  "engines": {
    "node": ">=20.0.0"
  }
}
