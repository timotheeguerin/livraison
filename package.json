{
  "name": "livraison",
  "version": "0.0.1",
  "type": "module",
  "description": "",
  "packageManager": "pnpm@10.5.2",
  "license": "MIT",
  "author": "Timothee Guerin",
  "keywords": [],
  "bin": {
    "livraison": "cmd/cli.js"
  },
  "files": [
    "cmd/cli.js",
    "dist/",
    "target/wasm32-wasip1/release/livraison.wasm"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "watch": "tsc -p ./tsconfig.build.json --watch",
    "build": "pnpm run build:rust && pnpm run build:ts",
    "build:ts": "cargo build --package livraison --target wasm32-wasip1 --release && tsc -p ./tsconfig.build.json",
    "build:rust": "cargo build",
    "clean": "rimraf dist/ .temp/",
    "test": "pnpm test:rust && pnpm test:ts",
    "test:ts": "vitest run",
    "test:rust": "cargo test",
    "test:watch": "vitest -w",
    "lint": "eslint . && cargo clippy -- -D warnings",
    "lint:sp": "syncpack lint",
    "format": "prettier . --write && syncpack format && cargo fmt",
    "format:check": "prettier . --check"
  },
  "dependencies": {},
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/cross-spawn": "^6.0.6",
    "@types/node": "^22.13.14",
    "@types/which": "^3.0.4",
    "@types/yargs": "^17.0.33",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vitest/ui": "^3.2.4",
    "cspell": "^9.2.0",
    "eslint": "^9.31.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unicorn": "^60.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "syncpack": "^13.0.4",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
