{
  "name": "livraison",
  "version": "0.1.0",
  "type": "module",
  "description": "",
  "packageManager": "pnpm@10.5.2",
  "license": "MIT",
  "author": "Timothee Guerin",
  "keywords": [],
  "main": "dist/index.js",
  "bin": {
    "livraison": "cmd/cli.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "watch": "tsc -p ./tsconfig.build.json --watch",
    "build": "pnpm run build:rust && pnpm run build:ts",
    "build:ts": "tsc -p ./tsconfig.build.json",
    "build:rust": "cargo build",
    "clean": "rimraf dist/ .temp/",
    "test": "pnpm test:rust && pnpm test:ts",
    "test:ts": "vitest run",
    "test:rust": "cargo test",
    "test:watch": "vitest -w",
    "lint": "eslint . && cargo clippy -- -D warnings",
    "lint:sp": "syncpack lint",
    "format": "prettier . --write && syncpack format && cargo fmt",
    "format:check": "prettier . --check"
  },
  "dependencies": {
    "cross-spawn": "^7.0.6",
    "is-unicode-supported": "^2.1.0",
    "pathe": "^2.0.3",
    "picocolors": "^1.1.1",
    "postject": "1.0.0-alpha.6",
    "std-env": "^3.8.1",
    "which": "^5.0.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/cross-spawn": "^6.0.6",
    "@types/node": "^22.13.14",
    "@types/which": "^3.0.4",
    "@types/yargs": "^17.0.33",
    "@vitest/eslint-plugin": "^1.1.38",
    "@vitest/ui": "^3.1.1",
    "cspell": "^8.18.1",
    "eslint": "^9.24.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "syncpack": "^13.0.3",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.30.0",
    "vitest": "^3.1.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
